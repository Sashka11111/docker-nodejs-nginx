generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "debian-openssl-3.0.x"]
}


datasource db {
  provider = "postgresql"
  url      = env("APP_PG_DATABASE_URL")
}

model Resource {
  id        String   @id @default(uuid())
  name      String
  type      String
  amount    Int
  price     Float
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
